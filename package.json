{
  "name": "preem-react-template",
  "version": "1.0.0",
  "description": "A Preem starter template for React applications.",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run clean && npm run security && npm run lint && npm run webpack:dev",
    "clean": "rimraf dist",
    "lint": "tslint -p ./",
    "open:dev": "npm run build:dev && webpack-dev-server --config webpack.config.dev.js --open",
    "security": "nsp check --reporter summary",
    "start": "npm run open:dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack:dev": "webpack --config webpack.config.dev.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bootstrap": "^4.1.0",
    "@types/react": "^16.3.5",
    "@types/react-css-modules": "^4.6.1",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.16",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.13",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "autoprefixer": "^8.2.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.8.3",
    "nsp": "^3.2.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "rimraf": "2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "ts-loader": "4.1.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.5.1",
    "typescript": "2.8.1",
    "webpack": "4.5.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "bootstrap": "^4.1.0",
    "react": "16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
